{
  "name": "non-monorepo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e .ts",
    "run:file": "nodemon --exec \"node -r esbuild-register \" -e .ts",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "start": "node -r esbuild-register ./src/index.ts",
    "create-wallet": "node -r esbuild-register ./scripts/create-wallet.ts",
    "test": "jest --detectOpenHandles",
    "compile": "hardhat compile",
    "deploy": "hardhat deploy-zksync --script stableBankerPaymaster.js"
    
  },
  "dependencies": {
    "@types/node": "18.11.9",
    "@types/react": "18.2.0",
    "@types/react-dom": "^18.0.6",
    "coingecko-api-v3": "^0.0.26",
    "dotenv": "^16.3.1",
    "eslint": "8.27.0",
    "eslint-config-next": "^13.4.1",
    "next": "^13.4.1",
    "node-telegram-bot-api": "^0.61.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "4.8.4",
    "zksync": "^0.13.1"
  },
  "devDependencies": {
    "@kyberswap/ks-sdk-classic": "^1.0.3",
    "@types/node-telegram-bot-api": "^0.61.6",
    "esbuild": "^0.18.15",
    "esbuild-register": "^3.4.2",
    "nodemon": "^3.0.1",
    "turbo": "latest",
    "@matterlabs/hardhat-zksync-deploy": "^0.6.1",
    "@matterlabs/hardhat-zksync-solc": "^0.4.0",
    "@matterlabs/hardhat-zksync-verify": "^0.1.8",
    "@matterlabs/zksync-contracts": "^0.6.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@openzeppelin/contracts": "^4.9.1",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.4",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
